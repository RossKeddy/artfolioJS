<% include partials/header.ejs %>
<table class="table table-striped">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Profession</th>
        <th>City</th>

    </tr>
    <% for (var i=0; i < artists.length; i++) { %>
    <tr>
        <td><%= artists[i].firstname, artists.lastname %></td>
        <td><%= artists[i].lastname %></td>
        <td><%= artists[i].profession %></td>
        <td><%= artists[i].city %></td>

    </tr>
    <% } %>
</table>
